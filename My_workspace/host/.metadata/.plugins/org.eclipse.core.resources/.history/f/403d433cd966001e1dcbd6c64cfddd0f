#include <stdio.h>
#include <stdint.h>

typedef struct
{

} PACKET_t

int main(void)
{
	uint32_t packet_value;
	printf("Enter the 32bit packet value: 0x");
	scanf("%X",&packet_value);
	printf("The 32bit packet value is: 0x%X",packet_value);

	getchar();getchar();
	return 0;
}

