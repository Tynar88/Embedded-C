#include <stdio.h>
#include <stdint.h>



int main(void)
{
	uint32_t packet_value;
	printf("Enter the 32bit packet value: 0x");
	scanf("%X",&packet_value);
	printf("The 32bit packet value is: %X",packet_value);

	getchar();getchar();
	return 0;
}

